<div id="<%= scope %>" ng-controller="<%= scope %> as <%= scope %>">
    <div class="panel panel-flat">
        <div class="page-header-lg page-header-sm page-header">
            <%- include('../../7-plugins/templates/header/begin',{scope:scope}); %>
            <ul class="breadcrumb-elements visible-elements">
                <li dragonlanguage="">
                    <a ng-click="resumen_indicador_poa.exportXLS();"><i class="icon-file-excel"></i></a>
                </li>
                <li dragonlanguage="">
                    <a ng-click="resumen_indicador_poa.openmodalField('pdf');"><i class="icon-printer2"></i></a>
                </li>
            </ul>
            <%- include('../../7-plugins/templates/header/end',{scope:scope}); %>
        </div>
        <div class="subcontent">
            <div class="row scrollit">
                <table class="table table-bordered" style="margin-top: 30px; width: 2500px;">
                    <thead class="">
                    <tr height="20" class="centralizarth" style="background-color: <%=CONFIG.ui.theme.primary.indexOf("rgb") > -1 ? CONFIG.ui.theme.primary : '#' + CONFIG.ui.theme.primary%>; color: white; ">
                        <th rowspan="3" >Indicadores</th>
                        <th ng-show="resumen_indicador_poa.session.tipo_institucion == 1" rowspan="3">Proyecto/Producto</th>
                        <th ng-show="resumen_indicador_poa.session.tipo_institucion == 2" rowspan="3">Proyecto/Plan de Acción</th>
                        <th rowspan="3">Fuente</th>
                        <th rowspan="3">Método Calculo</th>
                        <th rowspan="3">Unidad / Escala de Medición</th>
                        <th rowspan="3">Dirección Meta</th>
                        <th rowspan="3">Medio Verificación</th>
                        <th rowspan="3" height="0">Descripción</th>
                        <th colspan="2" height="0">Línea Base</th>

                        <th colspan="{{resumen_indicador_poa.cantidad}}">Metas</th>
                    </tr>

                    <tr height="20" class="centralizarth" style="background-color: <%=CONFIG.ui.theme.primary.indexOf("rgb") > -1 ? CONFIG.ui.theme.primary : '#' + CONFIG.ui.theme.primary%>; color: white">
                        <th rowspan="2">Año</th>
                        <th rowspan="2">Valor</th>
                        <th colspan="{{resumen_indicador_poa.cantidad}}" style="text-align: center;">
                            Proyectadas / Alcanzadas / Diferencias
                        </th>
                    </tr>
                    <tr height="20" class="centralizarth" style="background-color: <%=CONFIG.ui.theme.primary.indexOf("rgb") > -1 ? CONFIG.ui.theme.primary : '#' + CONFIG.ui.theme.primary%>; color: white">
                        <th ng-repeat="(column,value) in resumen_indicador_poa.headerBody_list">
                            {{value.name}}
                        </th>
                    </tr>
                    </thead>
                    <tbody  class="tb1" >
                    <tr ng-repeat="(key,value) in resumen_indicador_poa.resumen_indicador_poa_list">
                        <td rowspan="">{{value.indicadores}}</td>
                        <td rowspan="">{{value.producto}}</td>
                        <td rowspan="">{{value.fuente}}</td>
                        <td rowspan="">{{value.metodo_calculo}}</td>
                        <td rowspan="">{{value.tipo_meta}}</td>
                        <td rowspan="">{{value.direccion_meta}}</td>

                        <td rowspan="">{{value.medio_verificacion}}</td>
                        <td rowspan="">{{value.descripcion}}</td>
                        <td class="alinearNumeros">{{value.ano}}</td>
                        <td class="alinearNumeros"  rowspan="">{{value.linea_base}}</td>
                        <td class="" style="text-align: center;width: 8%;"  ng-repeat="(column,valuec) in resumen_indicador_poa.headerBody_list">
                            <span>{{value['periodo'+resumen_indicador_poa.headerBody_list[column].periodo+'P']}}</span> |
                            <span>{{value['periodo'+resumen_indicador_poa.headerBody_list[column].periodo+'A']}}</span> |
                            <span style="color: {{value['periodo'+resumen_indicador_poa.headerBody_list[column].periodo+'color']}}">
                                {{value['periodo'+resumen_indicador_poa.headerBody_list[column].periodo+'V']}}
                            </span>
                        </td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>

        <div class="subcontent" style="visibility: hidden">
            <table id="resumen_indicador_poaTable" class="table table-bordered">
                <thead class="">
                <tr class="" style="text-align: center;">
                    <th rowspan="3" >Indicadores</th>
                    <th rowspan="3">Proyecto/Producto</th>
                    <th rowspan="3">Fuente</th>
                    <th rowspan="3">Método Calculo</th>
                    <th rowspan="3">Unidad / Escala de Medición</th>
                    <th rowspan="3">Dirección Meta</th>
                    <th colspan="2" height="0">Línea Base</th>
                    <th rowspan="3">Medio Verificación</th>
                    <th rowspan="3" height="0">Descripción</th>
                    <th colspan="{{resumen_indicador_poa.cantidad * 3}}">Metas</th>
                </tr>
                <tr style="vertical-align: middle;">
                    <th rowspan="2">Año</th>
                    <th rowspan="2">valor</th>
                    <th colspan="3"  ng-repeat="(column,value) in resumen_indicador_poa.headerBody_list">
                        {{value.name}}
                    </th>
                </tr>
                <tr style="vertical-align: middle;">
                    <th ng-repeat="(key, value) in resumen_indicador_poa.arr_rs">{{resumen_indicador_poa.removeNumbers(value)}}</th>
                </tr>

                </thead>
                <tbody class="">
                <tr style="vertical-align: middle;" ng-repeat="(key,value) in resumen_indicador_poa.resumen_indicador_poa_list">
                    <td>{{value.indicadores}}</td>
                    <td>{{value.producto}}</td>
                    <td style="text-align: left;">{{value.fuente}}</td>
                    <td style="text-align: left;">{{value.metodo_calculo}}</td>
                    <td style="text-align: left;">{{value.tipo_meta}}</td>
                    <td>{{value.direccion_meta}}</td>
                    <td class="alinearNumeros">{{value.ano}}</td>
                    <td class="alinearNumeros"  rowspan="">{{value.linea_base}}</td>
                    <td>{{value.medio_verificacion}}</td>
                    <td>{{value.descripcion}}</td>
                    <td class="" style="text-align: center; vertical-align: middle;"   ng-repeat="(column,valuec) in value.periodo track by $index">
                        {{valuec}}
                    </td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
</div><!--end-->
