                <ul class="" style="margin-bottom: 0;">
                    <li class="" ng-repeat="value in foda_items.list_foda_items" repeat-end="foda_items.calculateEqualize();" ng-if="value.type === 3">
                        {{value.descripcion}} <a title="Alineado a ({{value.cantidad}}) estrategia/s" ng-click="foda_items.relateditems(value.id)">({{value.cantidad}})</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

