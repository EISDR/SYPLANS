<style>
    .form-group {
        margin-bottom: 10px;
        position: relative;
    }

    .chart canvas {
        cursor: pointer;
    }
</style>
<div id="<%= scope %>" ng-controller="<%= scope %> as <%= scope %>">
    <div class="panel panel-flat">
        <div class="page-header-lg page-header-sm page-header">
            <%- include('../../7-plugins/templates/header/begin',{scope:scope}); %>
            <ul class="breadcrumb-elements visible-elements">
                <li dragonlanguage=""></li>
            </ul>
            <%- include('../../7-plugins/templates/header/end',{scope:scope}); %>
        </div>
        <div class="subcontent">
            <div class="row">
                <div class="col-sm-12 col-md-12">
                    <ul class="nav nav-tabs text-center"
                        style="margin-bottom: 0px;margin-bottom: 15px;font-size: 15px;">

                        <li ng-class="[control_mando.selecteds.perspectiva===perspectiva.id?'active':'']"
                            ng-repeat="(i,perspectiva) in control_mando.perspectivas"
                            ng-click="control_mando.getEjes(perspectiva.id)">
                            <a data-toggle="tab" title="{{perspectiva.nombre}}" style="padding: 0;">
                            <span class="round-tabs one " style="text-transform: capitalize"
                                  ng-click="control_mando.getEjes(perspectiva.id)">
                                  <i title="{{perspectiva.nombre}}"
                                     ng-if="perspectiva.icono"
                                     class="text-primary {{perspectiva.icono}}"
                                     style="margin-top: 10px;font-size: 30px !important;">
                                  </i><h4 style="margin: 0 !important;">{{perspectiva[control_mando.name]}}</h4>
                            </span>
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="col-sm-12 col-md-12" style="padding: 0 10px 0 10px;">
                    <div>
                        <ul class="nav nav-primary nav-tabs primary nav-justified lostabs" style="display: none">
                            <li ng-repeat="(i,eje) in control_mando.ejes"
                                ng-click="control_mando.getObjetivos(eje.id)">
                                <a style="padding: 0" href="#EJE{{eje.id}}" data-toggle="tab" id="tabEJE{{eje.id}}"
                                   aria-expanded="false">
                                    <h6 style="margin: 2px; text-transform: none;"><b
                                                style="text-transform: uppercase;">Eje {{eje.no_orden}}:</b>
                                        {{eje.nombre}}</h6>
                                </a>
                            </li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane" ng-repeat="(i,eje) in control_mando.ejes" id="EJE{{eje.id}}">
                                <div class="panel panel-flat" style="padding: 10px 10px 0px 10px;margin-bottom: 0px;">
                                    <div class='col-sm-4 col-md-4'>
                                        <%- FORM.select('filter_objetivo', scope, {
                                            allownew: false,
                                            allowedit: false,
                                            allowview: false,
                                            allowlist: false,
                                            text: "item.nombre_objetivo",
                                            table: 'vw_cm_objetivos',
                                            label: "Objetivos Estratégicos",
                                            icon: {class: "archive"},
                                            condition: "(item.perspectiva==control_mando.selecteds.perspectiva) && (item.id != 0)"
                                        }) %>
                                    </div>
                                    <div class='col-sm-4 col-md-4' ng-if="!control_mando.session.institucion_id">
                                        <%- FORM.select('filter_departamento', scope, {
                                            allownew: false,
                                            allowedit: false,
                                            allowview: false,
                                            allowlist: false,
                                            text: "item.nombre",
                                            table: 'vw_departamento_filtro',
                                            label: "Departamento",
                                            icon: {class: "archive"},
                                            condition: "(item.compania==control_mando.session.compania_id && item.institucion==null) && (item.id != 0)"
                                        }) %>
                                    </div>
                                    <div class='col-sm-4 col-md-4' ng-if="control_mando.session.institucion_id">
                                        <%- FORM.select('filter_departamento', scope, {
                                            allownew: false,
                                            allowedit: false,
                                            allowview: false,
                                            allowlist: false,
                                            text: "item.nombre",
                                            table: 'vw_departamento_filtro',
                                            label: "Departamento",
                                            icon: {class: "archive"},
                                            condition: "item.institucion==control_mando.session.institucion_id"
                                        }) %>
                                    </div>
                                    <div class='col-sm-4 col-md-4'>
                                        <%- FORM.select('filter_usuario', scope, {
                                            allownew: false,
                                            allowedit: false,
                                            allowview: false,
                                            allowlist: false,
                                            text: "item.completo",
                                            table: 'vw_usuario',
                                            label: "Usuario",
                                            icon: {class: "archive"},
                                            condition: "(item.departamento==control_mando.filter_departamento)"
                                        }) %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12 col-md-12">
                    <div class="panel panel-flat" ng-show="control_mando.showtable && control_mando.productos.length">
                        <div class="panel-heading" style="background-color: #f7f7f7; padding: 5px 10px">
                            <strong ng-show="control_mando.session.tipo_institucion == 1" style="font-size: 18px">Desglose
                                de Proyecto/Producto</strong>
                            <strong ng-show="control_mando.session.tipo_institucion == 2" style="font-size: 18px">Desglose
                                de Planes de Acción</strong>
                        </div>
                        <div class="panel-body">
                            <div class="col-sm-12 col-md-12">
                                <button class="btn btn-primary"
                                        ng-disabled="control_mando.paginations.productos.currentPage == 0"
                                        ng-click="control_mando.paginations.productos.currentPage=control_mando.paginations.productos.currentPage-1">
                                    Página Anterior
                                </button>
                                <h4 style="display: inline-block;margin-right: 16px">
                                    Página {{control_mando.paginations.productos.currentPage+1}} de
                                    {{control_mando.paginations.pages(control_mando.paginations.productos)}}
                                </h4>
                                <button class="btn btn-primary"
                                        ng-disabled="control_mando.paginations.productos.currentPage >= control_mando.paginations.productos.data.length/control_mando.paginations.productos.pageSize - 1"
                                        ng-click="control_mando.paginations.productos.currentPage=control_mando.paginations.productos.currentPage+1">
                                    Siguiente Página
                                </button>
                            </div>
                            <div class='col-sm-4 col-md-4'
                                 ng-show="control_mando.paginations.show(control_mando.paginations.productos,key)"
                                 ng-repeat="(key, value) in control_mando.paginations.productos.data"
                                 repeat-end="control_mando.get_productos()">
                                <div class="" style="margin: 0">
                                    <div class="panel-body chart-container"
                                         style="padding: 10px 10px 20px 10px; cursor: pointer"
                                         ng-click="control_mando.open_view(value)">
                                        <div class=chart-container" style="height: 280px;">
                                            <h6 class="productos" title="{{value.nombre}}" style="text-align: center;">
                                                <strong>
                                                    {{control_mando.poco(value.nombre)}}
                                                </strong>
                                            </h6>


                                            <div class="chart has-fixed-height" id="gauge_productos_{{key}}"
                                                 style="height: 350px; cursor: pointer;"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-12 text-right">
                                <button class="btn btn-primary"
                                        ng-disabled="control_mando.paginations.productos.currentPage == 0"
                                        ng-click="control_mando.paginations.productos.currentPage=control_mando.paginations.productos.currentPage-1">
                                    Página Anterior
                                </button>
                                <h4 style="display: inline-block;margin-right: 16px">
                                    Página {{control_mando.paginations.productos.currentPage+1}} de
                                    {{control_mando.paginations.pages(control_mando.paginations.productos)}}
                                </h4>
                                <button class="btn btn-primary"
                                        ng-disabled="control_mando.paginations.productos.currentPage >= control_mando.paginations.productos.data.length/control_mando.paginations.productos.pageSize - 1"
                                        ng-click="control_mando.paginations.productos.currentPage=control_mando.paginations.productos.currentPage+1">
                                    Siguiente Página
                                </button>
                            </div>
                            <script>
                                control_mando.get_productos = async function () {
                                    control_mando.from_producto_actividades = await BASEAPI.listp('vw_gauge_actividad', {
                                        limit: 0
                                    });
                                    control_mando.from_producto_actividades = control_mando.from_producto_actividades.data;
                                    setTimeout(function () {
                                        control_mando.gauge_producto_list = [];
                                        control_mando.actividades_productos = [];
                                        for (let i = 0; i < control_mando.from_gauge_productos.length; i++) {
                                            debugger;
                                            control_mando.gauge_producto_list['gauge_productos' + i] = (echarts.init(document.getElementById('gauge_productos_' + i), 'infographic'));
                                            control_mando.actividades_productos[i] = control_mando.from_producto_actividades.filter(d => {
                                                return d.producto == control_mando.from_gauge_productos[i].id;
                                            })
                                            let total = 0;
                                            if (control_mando.actividades_productos[i].length > 0) {
                                                let cantidad = control_mando.actividades_productos[i].filter((item) => {
                                                    if (control_mando.filter_usuario !== '[NULL]')
                                                        if (control_mando.filter_usuario != item.responsable_id)
                                                            return false;
                                                    return true;
                                                }).length;
                                                let proyectadas = control_mando.actividades_productos[i].filter((item) => {
                                                    if (control_mando.filter_usuario !== '[NULL]')
                                                        if (control_mando.filter_usuario != item.responsable_id)
                                                            return false;
                                                    return item.condicion == 'Planificada';
                                                }).length;
                                                let ejecucion = control_mando.actividades_productos[i].filter((item) => {
                                                    if (control_mando.filter_usuario !== '[NULL]')
                                                        if (control_mando.filter_usuario != item.responsable_id)
                                                            return false;
                                                    return item.estado == 'En ejecución';
                                                }).length;
                                                let completadas = control_mando.actividades_productos[i].filter((item) => {
                                                    if (control_mando.filter_usuario !== '[NULL]')
                                                        if (control_mando.filter_usuario != item.responsable_id)
                                                            return false;
                                                    return item.estado == 'Completada' && item.condicion == 'Ninguno';
                                                }).length;
                                                total = ((proyectadas + ejecucion + completadas) / cantidad) || 0;
                                            } else {
                                                total = control_mando.from_gauge_productos[i].condition == 'Planificado' ? 1 : control_mando.from_gauge_productos[i].condition == 'En ejecución' ? 1 : control_mando.from_gauge_productos[i].estatus == 'Completado' ? 1 : 0;
                                            }
                                            let optiond = {
                                                darkMode: 'auto',
                                                graphics: {
                                                    elements: {
                                                        type: "group",
                                                        cursor: "pointer"
                                                    }
                                                },
                                                series: [{
                                                    type: 'gauge',
                                                    startAngle: 180,
                                                    endAngle: 0,
                                                    min: 0,
                                                    max: 1,
                                                    splitNumber: 10,
                                                    axisLine: {
                                                        lineStyle: {
                                                            width: 6,
                                                            shadowBlur: 4,
                                                            color: [
                                                                [0.20, '#db0d22'],
                                                                [0.40, '#db630d'],
                                                                [0.60, '#fff700'],
                                                                [0.80, '#1cfc08'],
                                                                [1, '#0c9900']
                                                            ]
                                                        }
                                                    },
                                                    pointer: {
                                                        icon: 'path://M12.8,0.7l12,40.1H0.7L12.8,0.7z',
                                                        length: '12%',
                                                        width: 20,
                                                        offsetCenter: [0, '-60%'],
                                                        itemStyle: {
                                                            color: 'auto'
                                                        }
                                                    },
                                                    axisTick: {
                                                        length: 12,
                                                        lineStyle: {
                                                            color: 'auto',
                                                            width: 2
                                                        }
                                                    },
                                                    splitLine: {
                                                        length: 20,
                                                        lineStyle: {
                                                            color: 'auto',
                                                            width: 5
                                                        }
                                                    },
                                                    axisLabel: {
                                                        color: 'black',
                                                        fontSize: 11,

                                                        distance: -100,
                                                        padding: [40, 15, 0, 17],
                                                        formatter: function (value) {
                                                            if (value === 0.9) {
                                                                return 'Excelente';
                                                            } else if (value === 0.7) {
                                                                return 'Bueno';
                                                            } else if (value === 0.5) {
                                                                return 'Regular';
                                                            } else if (value === 0.3) {
                                                                return 'Malo';
                                                            } else if (value === 0.1) {
                                                                return 'Muy Malo';
                                                            }
                                                        }
                                                    },
                                                    title: {
                                                        offsetCenter: [0, '-20%'],
                                                        fontSize: 30
                                                    },
                                                    detail: {
                                                        fontSize: 22,
                                                        offsetCenter: [0, '0%'],
                                                        valueAnimation: true,
                                                        formatter: function (value) {
                                                            return Math.round(value * 100) + '%';
                                                        },
                                                        borderWidth: 1,
                                                        color: 'black'
                                                    },
                                                    center: ["50%", "55%"],
                                                    data: [{value: total}],
                                                    radius: "85%"
                                                }]
                                            };
                                            control_mando.gauge_producto_list['gauge_productos' + i].setOption(optiond, true);
                                        }
                                        control_mando.calculate_productoEqualize();
                                    }, 200);
                                }
                            </script>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12 col-md-12" style="margin-top: -20px;">
                    <div class="panel panel-flat" ng-show="control_mando.showtable && control_mando.productos.length">
                        <div class="panel-heading" style="background-color: #f7f7f7; padding: 5px 10px">
                            <strong style="font-size: 18px">Desglose de Actividades</strong>
                        </div>
                        <div class="panel-body">
                            <div class="col-sm-12 col-md-12">
                                <button class="btn btn-primary"
                                        ng-disabled="control_mando.paginations.actividades.currentPage == 0"
                                        ng-click="control_mando.paginations.actividades.currentPage=control_mando.paginations.actividades.currentPage-1">
                                    Página Anterior
                                </button>
                                <h4 style="display: inline-block;margin-right: 16px">
                                    Página {{control_mando.paginations.actividades.currentPage+1}} de
                                    {{control_mando.paginations.pages(control_mando.paginations.actividades)}}
                                </h4>
                                <button class="btn btn-primary"
                                        ng-disabled="control_mando.paginations.actividades.currentPage >= control_mando.paginations.actividades.data.length/control_mando.paginations.actividades.pageSize - 1"
                                        ng-click="control_mando.paginations.actividades.currentPage=control_mando.paginations.actividades.currentPage+1">
                                    Siguiente Página
                                </button>
                            </div>
                            <div class='col-sm-4 col-md-4'
                                 style="height: 320px;{{value.color.replace('danger','border: solid 5px #f99790;box-sizing: border-box;').replace('success','border: solid 5px lightgreen;box-sizing: border-box;')}}"
                                 id="graficosactividades"
                                 ng-show="control_mando.paginations.show(control_mando.paginations.actividades,key)"
                                 ng-repeat="(key, value) in control_mando.paginations.actividades.data"
                                 repeat-end="control_mando.get_actividades()">
                                <div class="" style="margin: 0">
                                    <div class="panel-body chart-container"
                                         style="padding: 10px 10px 20px 10px; cursor: pointer"
                                         ng-click="control_mando.open_view_actividades(value)">
                                        <div class=chart-container" style="height: 280px">
                                            <h6 class="actividades" title="{{value.actividad}}"
                                                style="text-align: center">
                                                <strong>
                                                    {{value.no2}} {{control_mando.poco(value.actividad)}}
                                                </strong>
                                            </h6>
                                            <div class="chart has-fixed-height" id="gauge_actividades_{{key}}"
                                                 style="height: 350px;"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-12 col-md-12 text-right">
                                <button class="btn btn-primary"
                                        ng-disabled="control_mando.paginations.actividades.currentPage == 0"
                                        ng-click="control_mando.paginations.actividades.currentPage=control_mando.paginations.actividades.currentPage-1">
                                    Página Anterior
                                </button>
                                <h4 style="display: inline-block;margin-right: 16px">
                                    Página {{control_mando.paginations.actividades.currentPage+1}} de
                                    {{control_mando.paginations.pages(control_mando.paginations.actividades)}}
                                </h4>
                                <button class="btn btn-primary"
                                        ng-disabled="control_mando.paginations.actividades.currentPage >= control_mando.paginations.actividades.data.length/control_mando.paginations.actividades.pageSize - 1"
                                        ng-click="control_mando.paginations.actividades.currentPage=control_mando.paginations.actividades.currentPage+1">
                                    Siguiente Página
                                </button>
                            </div>
                            <script>
                                control_mando.get_actividades = async function () {
                                    control_mando.from_actividades_apoyo = await BASEAPI.listp('vw_actividades_apoyo', {
                                        limit: 0
                                    });
                                    control_mando.from_actividades_apoyo = control_mando.from_actividades_apoyo.data;
                                    setTimeout(function () {
                                        control_mando.gauge_actividades_list = [];
                                        control_mando.actividades_act_apoyo = [];
                                        for (let i = 0; i < control_mando.from_gauge_actividades.length; i++) {
                                            control_mando.gauge_actividades_list['gauge_actividades_' + i] = (echarts.init(document.getElementById('gauge_actividades_' + i), 'infographic'));
                                            control_mando.actividades_act_apoyo[i] = control_mando.from_actividades_apoyo.filter(d => {
                                                return d.actividades_poa == control_mando.from_gauge_actividades[i].id;
                                            })
                                            let total = 0;
                                            if (control_mando.actividades_act_apoyo[i].length > 0) {
                                                let cantidad = control_mando.actividades_act_apoyo[i].length;
                                                let planificadas = control_mando.actividades_act_apoyo[i].filter((item) => {
                                                    if (control_mando.filter_usuario !== '[NULL]')
                                                        if (control_mando.filter_usuario != item.responsable)
                                                            return false;
                                                    return item.condition == 'Planificada';
                                                }).length;
                                                let ejecucion = control_mando.actividades_act_apoyo[i].filter((item) => {
                                                    if (control_mando.filter_usuario !== '[NULL]')
                                                        if (control_mando.filter_usuario != item.responsable)
                                                            return false;
                                                    return item.condition == 'En ejecución';
                                                }).length;
                                                let completadas = control_mando.actividades_act_apoyo[i].filter((item) => {
                                                    if (control_mando.filter_usuario !== '[NULL]')
                                                        if (control_mando.filter_usuario != item.responsable)
                                                            return false;
                                                    return item.estatus == 2 && item.condition == 'Ninguno';
                                                }).length;
                                                total = (planificadas + ejecucion + completadas) / cantidad;
                                            } else {
                                                total = control_mando.from_gauge_actividades[i].condition == 'Planificada' ? 1 : control_mando.from_gauge_actividades[i].condition == 'En ejecución' ? 1 : control_mando.from_gauge_actividades[i].estatus == 'Completada' ? 1 : 0;
                                            }
                                            let optiond = {
                                                darkMode: 'auto',
                                                graphics: {
                                                    elements: {
                                                        type: "group",
                                                        cursor: "pointer"
                                                    }
                                                },
                                                series: [{
                                                    type: 'gauge',
                                                    startAngle: 180,
                                                    endAngle: 0,
                                                    min: 0,
                                                    max: 1,
                                                    splitNumber: 10,
                                                    axisLine: {
                                                        lineStyle: {
                                                            width: 6,
                                                            shadowBlur: 4,
                                                            color: [
                                                                [0.20, '#db0d22'],
                                                                [0.40, '#db630d'],
                                                                [0.60, '#fff700'],
                                                                [0.80, '#1cfc08'],
                                                                [1, '#0c9900']
                                                            ]
                                                        }
                                                    },
                                                    pointer: {
                                                        icon: 'path://M12.8,0.7l12,40.1H0.7L12.8,0.7z',
                                                        length: '12%',
                                                        width: 20,
                                                        offsetCenter: [0, '-60%'],
                                                        itemStyle: {
                                                            color: 'auto'
                                                        }
                                                    },
                                                    axisTick: {
                                                        length: 12,
                                                        lineStyle: {
                                                            color: 'auto',
                                                            width: 2
                                                        }
                                                    },
                                                    splitLine: {
                                                        length: 20,
                                                        lineStyle: {
                                                            color: 'auto',
                                                            width: 5
                                                        }
                                                    },
                                                    axisLabel: {
                                                        color: 'black',
                                                        fontSize: 11,

                                                        distance: -100,
                                                        padding: [40, 15, 0, 17],
                                                        formatter: function (value) {
                                                            if (value === 0.9) {
                                                                return 'Excelente';
                                                            } else if (value === 0.7) {
                                                                return 'Bueno';
                                                            } else if (value === 0.5) {
                                                                return 'Regular';
                                                            } else if (value === 0.3) {
                                                                return 'Malo';
                                                            } else if (value === 0.1) {
                                                                return 'Muy Malo';
                                                            }
                                                        }
                                                    },
                                                    title: {
                                                        offsetCenter: [0, '-20%'],
                                                        fontSize: 30
                                                    },
                                                    detail: {
                                                        fontSize: 22,
                                                        offsetCenter: [0, '0%'],
                                                        valueAnimation: true,
                                                        formatter: function (value) {
                                                            return Math.round(value * 100) + '%';
                                                        },
                                                        borderWidth: 1,
                                                        color: 'black'
                                                    },
                                                    center: ["50%", "55%"],
                                                    data: [{value: total}],
                                                    radius: "85%"
                                                }]
                                            };
                                            control_mando.gauge_actividades_list['gauge_actividades_' + i].setOption(optiond, true);
                                        }
                                        control_mando.calculate_actividadEqualize();
                                    }, 200);
                                }
                            </script>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12 col-md-12" ng-show="control_mando.showtable">
                    <div class="scrollit">
                        <div dragonlanguage=""
                             style="margin-top: 15px !important;"
                             ng-show="!control_mando.productos.length"
                             class="alert alpha-<%= COLOR.secundary %> border-<%= COLOR.secundary %> alert-styled-left">
                            <language>MESSAGE.ic('table.nodata')</language>
                        </div>
                        <div class="" ng-show="control_mando.productos.length">
                            <div class="page-header-lg page-header-sm page-header"
                                 style="margin-bottom: -15px !important;">
                                <%- include('../../7-plugins/templates/header/begin',{scope:scope}); %>
                                <i class="icon-lifebuoy" style="top: -3px; font-size: 15px; margin-right: 5px"></i>
                                <strong ng-show="control_mando.session.tipo_institucion == 1" style="font-size: 18px;"> Listado de Proyecto/Producto y Actividades </strong>
                                <strong ng-show="control_mando.session.tipo_institucion == 2"
                                        style="font-size: 18px;"> Listado de Proyecto/Plan de Acción y
                                    Actividades </strong>
                                <%- include('../../7-plugins/templates/header/end',{scope:scope}); %>
