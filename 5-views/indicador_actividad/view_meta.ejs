<table class="table table-bordered"style="width: 70%; margin-left: 10%">
    <thead>
        <tr style="background-color: <%=CONFIG.ui.theme.primary.indexOf("rgb") > -1 ? CONFIG.ui.theme.primary : '#' + CONFIG.ui.theme.primary%> !important; color: white; " dragonlanguage="">
            <th class="">
                <span> <language>Periodo</language> ({{indicador_actividad.dataForViewMeta.poa_monitoreo_nombre}})</span>
            </th>
            <th class="" style="width: 20%">
                <span> <language>Proyectado</language> </span>
            </th>
            <th class="" style="width: 20%">
                <span> <language>Alcanzado</language> </span>
            </th>
        </tr>
    </thead>
    <tbody>
    <tr ng-repeat="(keycolum, value) in <%= scope %>.list_indicador_actividad_periodo_view track by keycolum" repeat-end="<%= scope %>.refreshAngular()">
        <td class="">
            <span>{{value.periodo}}</span>
        </td>
        <td>
            <span>{{value.valor}}</span>
        </td>
        <td>
            <span>{{value.valor_alcanzado_2}}</span>
        </td>
    </tr>

    </tbody>
</table>
<!--Table List-->
<script>
    <%= scope %>.refreshAngular();
    setTimeout(function () {
        <%= scope %>.refreshAngular();
    },100);

</script>