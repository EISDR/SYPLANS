<div id="<%= scope %>" ng-controller="<%= scope %> as <%= scope %>">
    <div class="panel panel-flat">
        <%- include('../../7-plugins/templates/table/header',{scope:scope}); %>
        <div class="subcontent">
            <div class="row">
                <div class="col-md-6">
                    <label class=""> Niveles de Riesgo </label>
                    <ul id="leyendas" class="list-group" style="padding: 24px 0;">
                        <li ng-repeat="(key,value) in ser_salida.niveles | filter: {proceso: '0'}">
                            <label class="ponerRedondo" style="background-color: {{ value.color }}"></label>
                            <label class="labelRedondo"> {{value.nombre}} </label>
                        </li>
                    </ul>
                    <br>
                </div>
                <div class="col-md-6">
                    <label class=""> Niveles de Riesgo asociados a Procesos</label>
                    <ul id="leyendas" class="list-group" style="padding: 24px 0;">
                        <li ng-repeat="(key,value) in ser_salida.niveles | filter: {proceso: '1'}">
                            <label class="ponerRedondo" style="background-color: {{ value.color }}"></label>
                            <label class="labelRedondo"> {{value.nombre}} </label>
                        </li>
                    </ul>
                    <br>
                </div>
            </div>
            <%- include('../../7-plugins/templates/table/base',{scope:scope}); %>
        </div>
    </div>
</div><!--end-->