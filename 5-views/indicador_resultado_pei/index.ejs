<style>
    /*#tb-custom .percent, .money {*/
    /*top: 11px !important;*/
    /*}*/

    .money {
        top: 11px !important;
    }

    #tb-custom .has-feedback label ~ .form-control-feedback {
        top: 25px;
    }

    .tbodyDetails .legitRipple {
        height: 40px !important;
    }
</style>
<div id="<%= scope %>" ng-controller="<%= scope %> as <%= scope %>">
    <div class="panel panel-flat grayPanel">
        <div class="page-header-lg page-header-sm page-header">
            <%- include('../../7-plugins/templates/header/begin',{scope:scope}); %>
            <ul class="breadcrumb-elements visibhttp://localhost:8091/le-elements">
                <li dragonlanguage="">
                </li>
            </ul>
            <%- include('../../7-plugins/templates/header/end',{scope:scope}); %>

        </div>

        <div class="subcontent">
            <div class="row" ng-show="!indicador_resultado_pei.queries.id">
                <button
                        ng-show=" indicador_resultado_pei.asbierto"
                        dragonlanguage="" title="MESSAGE.ic('mono.cancel')"
                        type="button" class="btn bg-<%= COLOR.warning %> btn-labeled btn-xs pull-right"
                        ng-click="indicador_resultado_pei.cancel_pei_ano();">
                    <b><i class="icon-cross2"></i></b>
                    <language>MESSAGE.ic('mono.cancel')</language>
                </button>
                <button
                        ng-show=" indicador_resultado_pei.asbierto"
                        dragonlanguage="" title="MESSAGE.ic('mono.save')"
                        type="button" role="button"
                        class=" btn bg-<%= COLOR.success %>-800 btn-labeled btn-xs pull-right"
                        ng-click="indicador_resultado_pei.save_pei_ano();">
                    <b><i class="icon-floppy-disk"></i></b>
                    <language>MESSAGE.ic('mono.save')</language>
                </button>
                <div class="col-md-12" style="margin-top: 15px;">
                    <div class="panel panel-<%= COLOR.secundary %>">
                        <div class="panel-body">
                            <div class="row set-label" dragonlanguage="">
                                <span>
                                    <h6 style="margin-bottom: 18px;border-bottom-width: 0px;border-bottom-style: solid;padding-bottom: 0px;margin-top: -15px;border-right-width: 0px;border-right-style: solid;border-top-width: 0px;border-top-style: solid;margin-right: 0px;"><language>Filtrar por</language></h6>
                                </span>
                                <div class="col-sm-6 col-md-6">
                                    <%- FORM.select("resultado", scope, {
                                        allownew: false,
                                        allowedit: false,
                                        allowview: false,
                                        allowlist: false,
                                        refresh: false,
                                        text: "item.nombre",
                                        table: "drp_resultado_indicador_pei",
                                        icon: {class: "archive"},
                                        condition: 'indicador_resultado_pei.conditionPei.estatus && item.pei == indicador_resultado_pei.conditionPei.pei_id'
                                    }) %>
                                </div>
                                <div class="col-sm-6 col-md-6">
                                    <%- FORM.textarea("estrategia_resultado", scope, {
                                        disabled: true
                                    }) %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-<%= COLOR.secundary %>">
                        <div class="panel-body">
                            <div class="col-sm-6 col-md-6" ng-show="indicador_resultado_pei.queries.id">
                                <%- FORM.inputview("indicador_pei_object.nombre", scope, {}) %>
                            </div>
                            <div class="row" ng-show="!indicador_resultado_pei.queries.id">
                                <div class="col-sm-6 col-md-6">
                                    <%- FORM.select("indicador_pei", scope, {
                                        allownew: false,
                                        allowedit: false,
                                        allowview: false,
                                        allowlist: false,
                                        refresh: false,
                                        text: "item.nombre",
                                        table: "vw_indicador_pei_drp",
                                        icon: {class: "archive"},
                                        condition: "item.pei==indicador_resultado_pei.userVar.pei_id &&(item.resultado == indicador_resultado_pei.resultado || indicador_resultado_pei.resultado==='[NULL]')"
                                    }) %>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-12 col-md-12">
                                    <div class="col-sm-4 col-md-4" dragonlanguag="">
                                        <label class="control-label is-visible animate ng-binding"><language>Eje
                                            Estratégico</language></label>
                                        <textarea
                                                style="padding-top: 0px !important;  margin-bottom: 20px;"
                                                id="s" disabled class="form-control" maxlength="255" rows="4"
                                                ng-model="indicador_resultado_pei.eje_estrategico"></textarea>
                                    </div>
                                    <div class="col-sm-4 col-md-4" dragonlanguage="">
                                        <label class="control-label is-visible animate ng-binding"><language>Objetivo
                                            Estratégico</language></label>
                                        <textarea
                                                style="padding-top: 0px !important; margin-bottom: 20px;"
                                                id="ss" disabled class="form-control" maxlength="255" rows="4"
                                                ng-model="indicador_resultado_pei.objetivo_estrategico">

                                        </textarea>
                                    </div>
                                    <div class="col-sm-4 col-md-4" dragonlanguage="">
                                        <label class="control-label is-visible animate ng-binding"><language>Estrategia</language></label>
                                        <textarea
                                                style="padding-top: 0px !important;  margin-bottom: 20px;"
                                                id="sss" disabled class="form-control" maxlength="255" rows="4"
                                                ng-model="indicador_resultado_pei.estrategia"></textarea>
                                    </div>
                                </div>
                            </div>
                            <div class="row" style="margin-bottom: 10px">
                                <div class="col-sm-12 col-md-12">
                                    <label class="control-label is-visible animate ng-binding" dragonlanguage=""><language>Descripción Indicador
                                        PEI</language></label>
                                    <textarea
                                            id="descripcion" disabled="false" class="form-control" maxlength="4000"
                                            ng-model="indicador_resultado_pei.descripcion"></textarea>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-6 col-md-6">
                                    <label class="control-label is-visible animate ng-binding" dragonlanguage=""><language>Fuente</language></label>
                                    <textarea
                                            id="fuente" disabled="false" class="form-control" maxlength="255"
                                            ng-model="indicador_resultado_pei.fuente"></textarea>
                                </div>
                                <div class="col-sm-6 col-md-6">
                                    <label class="control-label is-visible animate ng-binding" dragonlanguage=""><language>Método de Cálculo</language></label>
                                    <textarea
                                            id="metodo_calculo" disabled="false" class="form-control" maxlength="255"
                                            ng-model="indicador_resultado_pei.metodo_calculo"></textarea>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-3 col-md-3">
                                    <%- FORM.inputview("tipo_meta_nombre", scope, {}) %>
                                </div>
                                <div class="col-sm-3 col-md-3">
                                    <%- FORM.inputview("direccion_meta_nombre", scope, {}) %>
                                </div>
                                <div class="col-sm-2 col-md-2">
                                    <%- FORM.inputview("ano_linea_base", scope, {}) %>
                                </div>
                                <div class="col-sm-2 col-md-2">
                                    <%- FORM.inputview("linea_base", scope, {}) %>
                                </div>
                                <div class="col-sm-2 col-md-2">
                                    <%- FORM.inputview("periodicidad", scope, {}) %>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-12 col-md-12">
                                    <label class="control-label is-visible animate ng-binding" dragonlanguage=""><language>Medio de
                                        Verificación</language></label>
                                    <textarea
                                            id="medio_verificacion" disabled="false" class="form-control"
                                            maxlength="4000"
                                            ng-model="indicador_resultado_pei.medio_verificacion"></textarea>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer" ng-show="" dragonlanguage=""
                 style="padding-top: 15px;"
                 ng-if="indicador_resultado_pei.asbierto && !indicador_resultado_pei.queries.id"
                 ng-show="(!indicador_resultado_pei.queries.id && (indicador_resultado_pei.estatus_pei == 4 && indicador_resultado_pei.condicion_pei == 1) )">
                <span class="label label-white text-<%= TAG.table %>-300 label-rounded label-icon">
                <i class="position-right"></i>
                </span>
                <button
                        ng-show=" indicador_resultado_pei.asbierto"
                        dragonlanguage="" title="MESSAGE.ic('mono.cancel')"
                        type="button" class="btn bg-<%= COLOR.warning %> btn-labeled btn-xs pull-right"
                        ng-click="indicador_resultado_pei.cancel_pei_ano();">
                    <b><i class="icon-cross2"></i></b>
                    <language>MESSAGE.ic('mono.cancel')</language>
                </button>
                <button
                        ng-show=" indicador_resultado_pei.asbierto"
                        dragonlanguage="" title="MESSAGE.ic('mono.save')"
                        type="button" role="button"
                        class=" btn bg-<%= COLOR.success %>-800 btn-labeled btn-xs pull-right"
                        ng-click="indicador_resultado_pei.save_pei_ano();">
                    <b><i class="icon-floppy-disk"></i></b>
                    <language>MESSAGE.ic('mono.save')</language>
                </button>

            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-<%= COLOR.secundary %>">
                        <div class="panel-body" id="tb-custom">
                            <div class="row over-table">
                                <table ng-show="indicador_resultado_pei.loaded" class="table table-bordered">
                                    <thead class="">
                                    <tr class="bg-{{indicador_resultado_pei.colors}}">
                                        <th style="text-align: center; vertical-align: middle;width: 5%" height="0">
                                            <language>Año</language>
                                        </th>
                                        <th style="text-align: center; vertical-align: middle;width: 5%" height="0">
                                            <language>Meta</language>
                                        </th>
                                        <th style="text-align: center;width: 15%; vertical-align: middle" height="0">
                                            <language>Meta Alcanzada</language>
                                        </th>
                                        <!--                                        <th style="text-align: center; vertical-align: middle;width: 5%" height="0">-->
                                        <!--                                            Diferencia-->
                                        <!--                                        </th>-->
                                        <th style="text-align: center; width: 60%; vertical-align: middle" height="0"
                                            ng-show="!indicador_resultado_pei.queries.id">
                                            <language>Observaciones</language>
                                        </th>
                                        <th ng-show="!indicador_resultado_pei.queries.id"
                                            style="text-align: center; vertical-align: middle;width: 15%"><language>Acciones</language>
                                        </th>
                                    </tr>
                                    </thead>
                                    <tbody id="tb" class="tb1 tbodyDetails">

                                    <tr ng-repeat="(key,value) in indicador_resultado_pei.list_indicador_resultado_pei"
                                        repeat-end="indicador_resultado_pei.applyMasks()" style="height: 130px">
                                        <td style="text-align: center; vertical-align: middle;">{{value.ano}}</td>
                                        <td style="text-align: right; vertical-align: middle;" class="meta">
                                            {{indicador_resultado_pei.formatear(value.valor)}}
                                        </td>
                                        <td style="text-align: right; vertical-align: middle;">


                                            <div class="subcontainer{{value.id}} acomodar" title="{{value.message}}">

                                            </div>
                                            <div style="    margin-top: -50px;"
                                                 ng-show="value.allow.allow===1">
                                                <a

                                                        title="Guardar"
                                                        style="height: 38px;width: 17px;background-color: transparent;cursor: pointer;"
                                                        class="savepartialy"
                                                        ng-click="indicador_resultado_pei.save_pei_ano(value.id);">
                                                    <i class="icon-floppy-disk text-secundary-200"
                                                       style=" color: #ccc !important;">

                                                    </i>
                                                </a>
                                                <a

                                                        title="Limpiar"
                                                        style="height: 38px;width: 17px;background-color: transparent;cursor: pointer;font-size: 22px; color: #ccc !important;"
                                                        class="savepartialy"
                                                        ng-click="indicador_resultado_pei.save_pei_ano(value.id,true);">
                                                    <i class="icon-rotate-ccw3 text-secundary-300"
                                                       style="font-size: 17px;margin-top: 5px;color: #ccc !important;margin-left: 10px;">

                                                    </i>
                                                </a>
                                            </div>
                                            <a data-history="indicador_resultado_pei"
                                               data-type="indicador_resultado_pei"
                                               data-id="{{value.id}}" data-toggle="tooltip" data-formula="${row.cambio}"
                                               data-label="Historial de cambios para el indicador {{indicador_resultado_pei.indicador_pei_object.nombre}}, Año {{value.ano}}"
                                               title="Historial"
                                               style="cursor: pointer;font-size: 22px; color: #ccc !important;float: left;margin-top: 32px"
                                               class="savepartialy">
                                                <i class="icon-sort-time-asc" style="font-size: 19px;"></i>
                                            </a>
                                        </td>
                                        <!--                                        <td style="text-align: right; vertical-align: middle;" class="variacion">-->
                                        <!--                                            {{indicador_resultado_pei.restar(value.id,-->
                                        <!--                                            value.valor==null?0:value.valor)}}-->
                                        <!--                                        </td>-->
                                        <td ng-show="!indicador_resultado_pei.queries.id"
                                            style="vertical-align: middle;{{value.allow.condition?'border: red outset':''}}">
                                            <div>
                                            <textarea
                                                    ng-show="(value.count_comment > 0)"
                                                    ng-readonly="true"
                                                    style="overflow: hidden; overflow-wrap: break-word; min-height: 80px; text-align: left;"
                                                    class="form-control no-border-readonly">{{value.comment}}</textarea>

                                            </div>
                                            <div style="float: right;margin-left: 14px;">
                                                <button title="Enviar notificación a Encargados y Analistas de la planificación"
                                                        type="button"
                                                        class="btn bg-warning btn-xs mr-0"
                                                        style="padding: 6px;"
                                                        ng-if="value.allow.condition==1 || value.allow.allow===1"
                                                        ng-click="indicador_resultado_pei.notificar(value.ano)">
                                                    <b><i class="icon-warning"></i></b>
                                                </button>
                                            </div>
                                            <div style="margin-top: 13px;">
                                                <a ng-show="value.count_comment > 0"
                                                   style="float: right;"
                                                   dragonlanguage=""
                                                   class="seeTextComplete metacomment{{$index}}"
                                                   ng-click="indicador_resultado_pei.openmodalField(0,value.ano,value.indicador_pei);">
                                                    <language>Ver todo</language> ({{value.count_comment}})
                                                </a>
                                                <a style="float: left" class="seeTextComplete"
                                                   dragonlanguage=""
                                                   ng-show="value.allow.allow===1"
                                                   ng-click="indicador_resultado_pei.openmodalField(1,value.ano,value.indicador_pei);">
                                                    <language>Agregar comentario</language>
                                                </a>
                                            </div>
                                        </td>
                                        <td ng-show="!indicador_resultado_pei.queries.id"
                                            style="text-align: center; vertical-align: middle;">

                                            <div class="permitido  modificar mollyindicadorpei" style="display: none"
                                                 dragoncontrol="file"
                                                 ng-if="value.allow.allow===1"
                                                 label="Subir Evidencias"
                                                 cols="12" name="IndicadorPEI{{value.id}}" scope="<%= scope %>"
                                                 id="IndicadorPEI{{value.id}}" cache="false">
                                                {
                                                maxfiles: 50,
                                                columns: 4,
                                                folder:"indicador_resultado_pei/META{{value.id}}",
                                                alone:true,
                                                }
                                            </div>

                                            <div class=" modificar mollyindicadorpei" style="display: none"
                                                 dragoncontrol="file"
                                                 ng-if="value.allow.allow!==1"
                                                 label="Ver Evidencias"
                                                 cols="12" name="IndicadorPEI{{value.id}}" scope="<%= scope %>"
                                                 id="IndicadorPEI{{value.id}}" cache="false">
                                                {
                                                maxfiles: 50,
                                                columns: 4,
                                                folder:"indicador_resultado_pei/META{{value.id}}",
                                                alone:true,
                                                }
                                            </div>

                                        </td>
                                    </tr>

                                    </tbody>
                                    <tfoot id="totals" ng-show="false">
                                        <tr class="dragon-columns" style="font-weight: bold">
                                            <td colspan="1" style="vertical-align: middle">Total</td>
                                            <td style="text-align: right;">
                                                {{indicador_resultado_pei.meta_total_desc}}
                                            </td>
                                            <td style="text-align: right;">
                                                {{indicador_resultado_pei.meta_total_alca}}
                                            </td>
                                            <td style="text-align: right;display: none" id="variacion">
                                                <i class="{{indicador_resultado_pei.iconfinal}}"></i>
                                                {{indicador_resultado_pei.getTotalVariacion()}}
                                            </td>
                                            <td colspan="1" style="vertical-align: middle">Cumplimiento
                                                <span title="{{(<%=scope%>.cumplidor||{}).ponderacion.titulo}}" style="padding: 10px;border-radius: 20px;color:white;font-weight: bold;background-color: {{(<%=scope%>.cumplidor||{}).ponderacion.color}}">
                                                    {{(<%=scope%>.cumplidor||{}).cumplimiento||0}}%
                                                </span>
                                            </td>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer" dragonlanguage=""
             style="padding-top: 15px;"
             ng-if="!indicador_resultado_pei.queries.id"
             ng-show="(!indicador_resultado_pei.queries.id && (indicador_resultado_pei.estatus_pei == 4 && indicador_resultado_pei.condicion_pei == 1)) || indicador_resultado_pei.asbierto">
                <span class="label label-white text-<%= TAG.table %>-300 label-rounded label-icon">
                <i class="position-right"></i>
                </span>
            <button
                    ng-show=" indicador_resultado_pei.asbierto"
                    dragonlanguage="" title="MESSAGE.ic('mono.cancel')"
                    type="button" class="btn bg-<%= COLOR.warning %> btn-labeled btn-xs pull-right"
                    ng-click="indicador_resultado_pei.cancel_pei_ano();">
                <b><i class="icon-cross2"></i></b>
                <language>MESSAGE.ic('mono.cancel')</language>
            </button>
            <button
                    ng-show=" indicador_resultado_pei.asbierto"
                    dragonlanguage="" title="MESSAGE.ic('mono.save')"
                    type="button" role="button" class=" btn bg-<%= COLOR.success %>-800 btn-labeled btn-xs pull-right"
                    ng-click="indicador_resultado_pei.save_pei_ano();">
                <b><i class="icon-floppy-disk"></i></b>
                <language>MESSAGE.ic('mono.save')</language>
            </button>

        </div>
    </div>
</div><!--end-->

<script>
    ;
</script>
