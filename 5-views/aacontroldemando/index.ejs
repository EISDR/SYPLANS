<div id="<%= scope %>" ng-controller="<%= scope %> as <%= scope %>">
    <%- include('angel/styles',{scope:scope}); %>
    <%- include('gaby/styles',{scope:scope}); %>
    <%- include('alam/styles',{scope:scope}); %>
    <%- include('angel/scriptheader',{scope:scope}); %>
    <%- include('gaby/scriptheader',{scope:scope}); %>
    <%- include('alam/scriptheader',{scope:scope}); %>
    <div class="panel panel-flat">
        <div class="page-header-lg page-header-sm page-header">
            <%- include('../../7-plugins/templates/header/begin',{scope:scope}); %>
            <ul class="breadcrumb-elements visible-elements" style="margin-top: 5px;">
                <li dragonlanguage="" style="margin-right: -10px;" title="Refrescar">
                    <a ng-click="<%= scope %>.refresh();">
                        <i class="icon-rotate-cw3"></i>
                    </a>
                </li>
            </ul>
            <%- include('../../7-plugins/templates/header/end',{scope:scope}); %>
        </div>


        <div class="subcontent text-center" id="tabsComment" style="    margin-top: 100px;"
             ng-if="aacontroldemandog.unicomenu">
            <div ng-if="!aacontroldemandog.aunnoexiste">
                <button style=" min-width: 700px; padding: 15px 15px 15px 15px; "
                        class="btn btn-primary btn-labeled btn-xs" ng-click="<%= scope %>.filtrar('cerrar')">
                    <i class="position-left"></i>{{aacontroldemandog.unicomenuTilte}} con Cálculos a la Fecha<i
                            class="border-white  position-right"></i>
                </button>
                <br>
                <button style=" min-width: 700px; padding: 15px 15px 15px 15px; margin-top: 100px;"
                        class="btn btn-primary btn-labeled btn-xs" ng-click="<%= scope %>.filtrar('abrir')">
                    <i class="position-left"></i>{{aacontroldemandog.unicomenuTilte}} con Cálculos de Períodos
                    Transcurridos (pasado)<i
                            class="border-white  position-right"></i>
                </button>
            </div>
            <div ng-if="aacontroldemandog.aunnoexiste">
                <div class="swal2-icon swal2-info swal2-animate-info-icon"
                     style="display: flex;width: 210px;height: 139px;padding-top: 72px !important;">
                    <span style="    font-size: 200px;" class="swal2-icon-text">i</span>
                </div>
                <h1>{{aacontroldemandog.aunnoexiste}}</h1>
            </div>
        </div>

        <div class="subcontent text-center" ng-if="!aacontroldemandog.unicomenu">

            <div class="container" style="width: 100%">
                <div class="col-sm-3 " style="text-align: right;margin-top: -1px" ng-show="aacontroldemando">
                    <h3>Evaluación filtrada por {{<%= scope %>.elpoa.nombre_mostrar}}(s):</h3>
                </div>
                <div class="col-sm-3 " style="text-align: right;margin-top: -1px" ng-show="aacontroldemando_pei">
                    <h3>Evaluación filtrada por año(s):</h3>
                </div>
                <div class="col-sm-8" style="text-align: left;margin-top: 8px;">
                    <select3 class="form-control fantizo" ng-model="<%= scope %>.filtrofecha"
                             multiple s2-options="opt.id as opt.name for opt in <%= scope %>.filters">
                    </select3>
                    <style>
                        .select3-container {
                            font-size: 14px !important;
                        }

                        .select3-container-multi .select3-choices {
                            min-height: 26px;
                            border: 3px solid #192649 !important;
                            /* text-align: center; */
                        }

                        .fantizo li.select3-search-choice div {
                            height: 33px;
                            padding-top: 10px;
                        }
                    </style>
                </div>
                <div class="col-sm-1 mt-5" style="text-align: right;text-align: right;margin-top: 17px !important;">
                    <button ng-click="<%= scope %>.setFiltro()" class="btn btn-primary">Filtrar</button>
                </div>
                <div class="col-sm-12" style="margin-top: 26px"
                     ng-if="!<%= scope %>.modolista && !aacontroldemandog.unicomenu">
                    <center>
                        <div class="panel panel-default"
                             style="height: 200px;width: 300px;margin-left: 25px;margin-top: 15px;">
                            <div class="panel-heading text-center"
                                 style="padding: 2px 0 0 8px; border-top-left-radius: 20px; border-top-right-radius: 20px;">
                                <h5 title="Cumplimiento General"
                                    style=" height: 26px; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;">
                                    <b>Cumplimiento General</b>
                                </h5>
                            </div>
                            <div class="panel-body text-center">
                                <div class="row" style="height: 50px;">
                                    <div id="graficoGeneral"
                                         style="text-align: center;float: right;margin: -15px -16px 0 0px;">
                                    </div>
                                    <h5 style="margin-top: -85px;margin-left: 90px;float: left;">
                                        {{<%= scope %>.generalPonderacion.titulo}}</h5>
                                </div>
                            </div>
                        </div>
                    </center>
                </div>
                <div class="containerGeneral" style="width: 100%;margin-top: 85px" ng-if="!aacontroldemandog.unicomenu">
                    <div class="row"
                         ng-repeat="(key,value) in <%= scope %>.mainMenu track by key"
                         ng-style="{'margin-top': (key > 0)? '2rem' : ''}">
                        <div class="col-sm-{{data.col||3}}"
                             ng-repeat="(id,data) in value.row">
                            <button class="btn btn-primary btn-labeled btn-xs"
                                    style="padding: 15px 15px 15px 15px; text-overflow: ellipsis; white-space: nowrap; overflow: hidden; min-width: 240px; max-width: 240px"
                                    ng-click="<%= scope %>.execute(data.link)">{{data.title}}
                            </button>
                        </div>

                    </div>

                </div>
            </div>
        </div>
    </div>
</div><!--end-->
<%- include('angel/scriptfooter',{scope:scope}); %>
<%- include('gaby/scriptfooter',{scope:scope}); %>
<%- include('alam/scriptfooter',{scope:scope}); %>
