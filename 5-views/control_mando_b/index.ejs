                                                        center: ["50%", "55%"],
                                                        data: [{value: total}],
                                                        radius: "85%"
                                                    }]
                                                };
                                                control_mando_b.gauge_actividades_list['gauge_actividades_' + i].setOption(optiond, true);
                                            }
                                            control_mando_b.calculate_actividadEqualize();
                                        }
                                    }, 500);
                                }
                            </script>
                        </div>
                    </div>
