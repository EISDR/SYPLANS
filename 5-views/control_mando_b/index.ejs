                                                        endAngle: 0,
                                                        min: 0,
                                                        max: 1,
                                                        splitNumber: 10,
                                                        axisLine: {
                                                            lineStyle: {
                                                                width: 6,
                                                                shadowBlur: 4,
                                                                color: [
                                                                    [0.20, '#db0d22'],
                                                                    [0.40, '#db630d'],
                                                                    [0.60, '#fff700'],
                                                                    [0.80, '#1cfc08'],
                                                                    [1, '#0c9900']
                                                                ]
                                                            }
                                                        },
                                                        pointer: {
                                                            icon: 'path://M12.8,0.7l12,40.1H0.7L12.8,0.7z',
                                                            length: '12%',
                                                            width: 20,
                                                            offsetCenter: [0, '-60%'],
                                                            itemStyle: {
                                                                color: 'auto'
                                                            }
                                                        },
                                                        axisTick: {
                                                            length: 12,
                                                            lineStyle: {
                                                                color: 'auto',
                                                                width: 2
                                                            }
                                                        },
                                                        splitLine: {
                                                            length: 20,
                                                            lineStyle: {
                                                                color: 'auto',
                                                                width: 5
                                                            }
                                                        },
                                                        axisLabel: {
                                                            color: 'black',
                                                            fontSize: 11,

                                                            distance: -100,
                                                            padding: [40, 15, 0, 17],
                                                            formatter: function (value) {
                                                                if (value === 0.9) {
                                                                    return 'Excelente';
                                                                } else if (value === 0.7) {
                                                                    return 'Bueno';
                                                                } else if (value === 0.5) {
                                                                    return 'Regular';
                                                                } else if (value === 0.3) {
                                                                    return 'Malo';
                                                                } else if (value === 0.1) {
                                                                    return 'Muy Malo';
                                                                }
                                                            }
                                                        },
                                                        title: {
                                                            offsetCenter: [0, '-20%'],
