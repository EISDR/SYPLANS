<div class="tab-content">
    <div class="tab-pane active" id="basic">
        <div class="row">
            <div class="col-sm-4 col-md-4">
                <%- FORM.select("objetivo", scope, {
                    allownew: false,
                    allowedit: false,
                    allowview: false,
                    allowlist: false,
                    refresh: false,
                    multiple: false,
                    truelabel: "Objetivo General",
                    text: "end.edt +'.'+ item.edt +' ' + item.nombre",
                    table: "objetivo",
                    condition: "item.end == end.id",
                    icon: {class: "archive"}
                }) %>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-2 col-md-2">
                <%- FORM.integer("edt", scope, {maxlength: 5, placeholder: "No. Secuencia"}) %>
            </div>
            <div class="col-sm-6 col-md-6">
                <%- FORM.input("nombre", scope, {maxlength: 1000}) %>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-12">
                <%-
                // FORM.input("descripcion", scope, {placeholder:"Descripción",maxlength: 255})
                FORM.textarea('descripcion', scope, {label: "Descripción", placeholder: "Descripción", maxlength: 1000})
                %>
            </div>
        </div>
    </div>
</div>
<%- FORM.footer(scope) %>
