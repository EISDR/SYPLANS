<div id="<%= scope %>" ng-controller="<%= scope %> as <%= scope %>">
    <div class="panel panel-flat">
        <%- include('../../7-plugins/templates/table/header',{scope:scope}); %>
        <div class="subcontent">
            <div class="row">
                <div class="col-sm-3 col-md-3" style="margin-top: 11px !important">
                    <%-FORM.select("entidad_view", scope, {
                        allownull: false,
                        allownew: false,
                        allowedit: false,
                        allowlist: false,
                        allowview: false,
                        refresh: false,
                        multiple: false,
                        text: "item.nombre",
                        value: "controller",
                        table: "auditoria_entidad_flujo",
                        field: "id",
                        icon: {class:"archive"},
                    // condition: "item.poa === actividades_poa.poa_id"
                    })%>
                </div>
                <br>
            </div>
            <%- include('../../7-plugins/templates/table/base',{scope:scope}); %>
        </div>
    </div>
</div><!--end-->
<script>
    $(function () {
        $('#<%= scope %>TablePanel').floatingScrollbar();
        setTimeout(function (){
            $("[name='auditoria_programa_plan_estatus_entidad_view']").change(function (val) {
                auditoria_programa_plan_estatus.changeIndicador($("[name='auditoria_programa_plan_estatus_entidad_view']").val());
            })
        }, 1000)
    });
</script>