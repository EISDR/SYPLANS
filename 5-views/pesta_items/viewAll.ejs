<div class="row" style="padding-left: 9px; padding-right: 9px;">
    <div class="col-sm-12 col-md-6">
        <div class="panel">
            <div class="panel-heading panel-dashboard bg-<%= COLOR.secundary %>">
                <h6 class="panel-title">Político</h6>
            </div>
            <div class="panel-body pesupuesto">
                <ul class="">
                    <li class="" ng-repeat="value in pesta_items.list_pesta_items" repeat-end="pesta_items.calculateEqualize()"  ng-if="value.type === 1">
                        {{value.descripcion}} <a title="Alineado a ({{value.cantidad}}) estrategia/s" ng-click="pesta_items.relateditems(value.id)">({{value.cantidad}})</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>

    <div class="col-sm-12 col-md-6">
        <div class="panel">
